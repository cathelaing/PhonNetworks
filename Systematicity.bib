@article{hills_longitudinal_2009,
	title = {Longitudinal Analysis of Early Semantic Networks Preferential Attachment or Preferential Acquisition?},
	volume = {20},
	issn = {0956-7976},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4216730/},
	doi = {10.1111/j.1467-9280.2009.02365.x},
	abstract = {Analyses of adult semantic networks suggest a learning mechanism involving preferential attachment: A word is more likely to enter the lexicon the more connected the known words to which it is related. We introduce and test two alternative growth principles: preferential acquisition—words enter the lexicon not because they are related to well-connected words, but because they connect well to other words in the learning environment—and the lure of the associates—new words are favored in proportion to their connections with known words. We tested these alternative principles using longitudinal analyses of developing networks of 130 nouns children learn prior to the age of 30 months. We tested both networks with links between words represented by features and networks with links represented by associations. The feature networks did not predict age of acquisition using any growth model. The associative networks grew by preferential acquisition, with the best model incorporating word frequency, number of phonological neighbors, and connectedness of the new word to words in the learning environment, as operationalized by connectedness to words typically acquired by the age of 30 months.},
	pages = {729--739},
	number = {6},
	journaltitle = {Psychological science},
	shortjournal = {Psychol Sci},
	author = {Hills, Thomas T. and Maouene, Mounir and Maouene, Josita and Sheya, Adam and Smith, Linda},
	urldate = {2020-06-11},
	date = {2009-06},
	pmid = {19470123},
	pmcid = {PMC4216730},
	file = {PubMed Central Full Text PDF:C\:\\Users\\Daniel & Catherine\\Zotero\\storage\\ULMKHXWQ\\Hills et al. - 2009 - Longitudinal Analysis of Early Semantic Networks P.pdf:application/pdf}
}

@article{fourtassi_growth_2020,
	title = {The Growth of Children's Semantic and Phonological Networks: Insight From 10 Languages},
	volume = {44},
	issn = {1551-6709},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1111/cogs.12847},
	doi = {10.1111/cogs.12847},
	shorttitle = {The Growth of Children's Semantic and Phonological Networks},
	abstract = {Children tend to produce words earlier when they are connected to a variety of other words along the phonological and semantic dimensions. Though these semantic and phonological connectivity effects have been extensively documented, little is known about their underlying developmental mechanism. One possibility is that learning is driven by lexical network growth where highly connected words in the child's early lexicon enable learning of similar words. Another possibility is that learning is driven by highly connected words in the external learning environment, instead of highly connected words in the early internal lexicon. The present study tests both scenarios systematically in both the phonological and semantic domains across 10 languages. We show that phonological and semantic connectivity in the learning environment drives growth in both production- and comprehension-based vocabularies, even controlling for word frequency and length. This pattern of findings suggests a word learning process where children harness their statistical learning abilities to detect and learn highly connected words in the learning environment.},
	pages = {e12847},
	number = {7},
	journaltitle = {Cognitive Science},
	author = {Fourtassi, Abdellah and Bian, Yuan and Frank, Michael C.},
	urldate = {2020-07-16},
	date = {2020},
	keywords = {Word learning, Cross-linguistic analysis, Network growth, Phonological network, Semantic network},
	file = {Full Text PDF:C\:\\Users\\Daniel & Catherine\\Zotero\\storage\\AIUN7MXK\\Fourtassi et al. - 2020 - The Growth of Children's Semantic and Phonological.pdf:application/pdf;Snapshot:C\:\\Users\\Daniel & Catherine\\Zotero\\storage\\6GM6S6ST\\cogs.html:text/html}
