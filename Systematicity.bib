@article{stoel-gammon_patterns_1984,
	title = {Patterns of early lexical and phonological development},
	volume = {11},
	issn = {0305-0009, 1469-7602},
	url = {https://www.cambridge.org/core/product/identifier/S0305000900005766/type/journal_article},
