@article{vihman_variable_1993,
	title = {Variable paths to early word production},
	volume = {21},
	issn = {00954470},
	abstract = {Explores variation in the beginnings of intentional communication, its manifestations, its sources, and its significance. Variability provides a basis for the selection of a path into language use. Illustration of the extent of variation is drawn from 20 (5 Ss per group) children learning 4 languages and compared with analyses of the mothers' speech for 1 group. The organization of early word production patterns into an initial phonological system is traced for one of the children. It is hypothesized that an articulatory filter based on a child's familiarity with the auditory consequences of his or her own vocal patterns provides a mechanism for the 1st production of speech forms; a mechanism supporting the shift from babble to intentional vocal communication is also suggested. ((c) 1997 {APA}/{PsycINFO}, all rights reserved)},
