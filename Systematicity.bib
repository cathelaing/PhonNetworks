@article{vihman_external_1994,
	title = {External Sources of Individual Differences? A Cross-Linguistic Analysis of the Phonetics of Mothers' Speech to 1-Year-Old Children},
	volume = {30},
	issn = {00121649},
	abstract = {Wide individual differences in early word production characterize children learning the same language, but the role of specific adult input in this interchild variability is unknown. Sampling the speech of American, French, and Swedish mothers (5 in each language group) to their 1-year-old children, this study analyzed the distribution of consonantal categories, word length, and final consonants in running speech, content words, initial consonant of content words, and target words (adult models of words attempted by the children) as well as the children's own early words (from age 9 months to about 18 months). Variability is greater in child words than adult speech, and individual mother-child dyads show no evidence of specific maternal influence on the phonetics of the child's speech.},
	pages = {651--662},
	number = {5},
